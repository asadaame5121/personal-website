<div class="daily-log">
  <h2>日報</h2>
  

  <!-- 既存の日報エントリ -->
  <div class="obsidian-logs">
    <h3>Obsidian日報</h3>
    {% for log in search.pages("url^=/obsidian/log/", "date=desc", 5) %}
    <div class="log-entry">
      <div class="log-date">{{ log.date | date("YYYY年MM月DD日") }}</div>
      <div class="log-content">{{ log.content | safe }}</div>
    </div>
    {% endfor %}
  </div>
</div>