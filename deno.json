{
  "imports": {
    "@atproto/api": "npm:@atproto/api@^0.14.22",
    "@std/dotenv": "jsr:@std/dotenv@^0.225.3",
    "@std/front-matter": "jsr:@std/front-matter@^1.0.9",
    "@std/fs": "jsr:@std/fs@^1.0.16",
    "fast-xml-parser": "npm:fast-xml-parser@^5.2.0",
    "lume/": "https://deno.land/x/lume@v3.0.4/",
    "lume/jsx-runtime": "https://deno.land/x/ssx@v0.1.9/jsx-runtime.ts"
  },
  "tasks": {
    "lume": "echo \"import 'lume/cli.ts'\" | deno run -A -",
    "build": "deno task lume",
    "serve": "deno task lume -s"
  },
  "compilerOptions": {
    "types": [
      "lume/types.ts"
    ],
    "jsx": "react-jsx",
    "jsxImportSource": "lume"
  }
}
